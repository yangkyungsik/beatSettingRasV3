    <string name="cmd_remove_bash_profile">rm test1.txt</string>
    <string-array name="cmd_init_bash_profile">
        <item>echo -e "JAVA_HOME=/usr/lib/jvm/jdk1.8.0_15" >> test1.txt\n</item>
        <item>echo -e "PATH='$PATH:$JAVA_HOME/bin'" >> test1.txt\n</item>
        <item>echo -e "\n" >> test1.txt</item>
        <item>echo -e "export JAVA_HOME PATH" >> test1.txt\n</item>
        <item>echo -e "export BOOTH_ID=!@#$" >> test1.txt\n</item>
        <item>echo -e "alias ll='ls -all'" >> test1.txt\n</item>
        <item>echo -e "export SPRING_PROFILES_ACTIVE=real" >> test1.txt\n</item>
        <item>cat test1.txt</item>
    </string-array>
    <string-array name="cmd_remove_bash_profile">
        <item>clear\n</item>
        <item>rm test1.txt\n</item>
    </string-array>
    <string-array name="cmd_read_bash_profile">
        <item>clear\n</item>
        <item>cat test1.txt\n</item>
    </string-array>
